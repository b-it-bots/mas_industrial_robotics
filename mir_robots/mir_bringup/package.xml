<?xml version="1.0"?>
<package>
  <name>mir_bringup</name>
  <version>0.0.1</version>
  <description>launch files to bring up a complete robot or only specific components</description>

  <maintainer email="frederik.hegger@h-brs.de">Frederik Hegger</maintainer>

  <license>GPLv3</license>

