  <run_depend>mcr_default_env_config</run_depend>
  <run_depend>mcr_collision_velocity_filter</run_depend>
  <run_depend>mir_arm_cartesian_control</run_depend>
  <run_depend>mir_default_robot_config</run_depend>
  <run_depend>mir_gripper_controller</run_depend>
