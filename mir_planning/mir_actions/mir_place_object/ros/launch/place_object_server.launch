<?xml version="1.0"?>
<launch>

    <arg name="max_allowed_tries" default= "3" />

    <node pkg="mir_place_object" type="place_object_server.py"
          name="place_object_server" output="screen">
        <rosparam param="max_allowed_tries" subst_value="True">
            $(arg max_allowed_tries)
        </rosparam>
    </node>

    <!-- launch place_pose_selector node to get platform heights-->
    <rosparam command="load" file="$(find mir_place_pose_selector)/ros/config/platform_heights.yaml"/>

</launch>
